package mesmaths.cinematique.brouillon;

/**
 * 
 * Lancée pour indiquer que pour un arc, une notion n'est pas définie.
 * 
 * Par exemple le projeté orthogonal d'un point quelconque sur un arc n'est pas toujours défini.
 * */
public class ArcException extends RuntimeException
{

    public ArcException()
    {
    super();
    }

    public ArcException(String arg0)
    {
    super(arg0);
    }

}
