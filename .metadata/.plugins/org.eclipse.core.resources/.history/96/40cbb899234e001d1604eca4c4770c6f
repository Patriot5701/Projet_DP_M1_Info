package musique.javax;

import javax.sound.sampled.FloatControl;


/**
 * classe Bidon qui est innstanciée lorsque le contrôle n'est pas disponible
 * 
 * sert uniquement à simplifier l'algo général dans les cas particuliers 
 * */
public class FloatControlFantôme extends FloatControl
{
public FloatControlFantôme()
{
super(null, Float.NaN, Float.NaN, Float.NaN, -1, Float.NaN, "");
}

@Override
public void setValue(float newValue)
{
/* on ne fait rien */
}

}
